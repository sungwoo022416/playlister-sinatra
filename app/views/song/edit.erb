<h1>Edit a New Song</h1>
<form action="/songs/<%= song.slug %>" method="POST">
<input id ="hidden" type="hidden" name="_method" value="PATCH">
<label for="artist_name">Artist Name</label> 
<input type="text" name="artist[name]" value = <%= @artist.name %> id="artist_name" > </br> 
<label for="name">Name</label>  
<input type="text" name="song[name]" value = <%= @song.name %> id="name"> </br>  
<label> Genres </label>  
<% Genre.all.each do |genre| %>    
    <label><%=genre.name %>
    <input id="<%= genre.name %>" type="checkbox" 
    name="genres[]" value="<%= genre.id %>"> </label>
<% end %>  
<input type="submit" value="Create">
</form>